{
	"endOfLine": "lf",
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"proseWrap": "always",
	"embeddedLanguageFormatting": "auto",
	"printWidth": 80,
	"quoteProps": "consistent",
	"plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
